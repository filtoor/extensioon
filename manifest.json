{
    "manifest_version": 2,
    "name": "Extensioon",
    "version": "1.0",
    "description": "An extension to filter spam nfts on popular exchanges",
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "256": "logo_trans.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  